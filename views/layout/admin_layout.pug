    body
        div
            .news-ticker.bg-orange
                .container
            #header-topbar-option-demo.page-header-topbar
                nav#topbar.navbar.navbar-default.navbar-static-top(role='navigation' style='margin-bottom: 0; z-index: 2;')
                    .navbar-header
                        button.navbar-toggle(type='button' data-toggle='collapse' data-target='.sidebar-collapse')
                            span.sr-only Toggle navigation
                            span.icon-bar
                            span.icon-bar
                            span.icon-bar
                        a#logo.navbar-brand(href='#')
                            span.fa.fa-rocket
                            span.logo-text Admin
                            span.logo-text-icon(style='display:none')
                    .topbar-main
                        a#menu-toggle.hidden-xs(href='#')
                            i.fa.fa-bars
                        form#topbar-search.hidden-xs(action="#", method="get")
                            .input-group
                                input.form-control(type='text' placeholder='Search...')
                                span.input-group-btn
                                    a.btn.submit(href='javascript:;')
                                        i.fa.fa-search
            #wrapper
                nav#sidebar.navbar-default.navbar-static-side(role='navigation')
                    .sidebar-collapse.menu-scroll
                        ul#side-menu.nav
                            li.user-panel
                                .thumb
                                    img(src="https://s3.amazonaws.com/uifaces/faces/twitter/kolage/128.jpg", alt="", srcset="").img-circle
                                .info
                                    p Best-Home
                                    ul.list-inline.list-unstyled
                                        li
                                            a(href="#")
                                                i.fa.fa-user
                                        li
                                            a(href="#")
                                                i.fa.fa-envelope
                                        li
                                            a(href="#")
                                                i.fa.fa-cog
                                        li
                                            a(href="/admin/logout")
                                                i.fa.fa-sign-out
                                .clearfix
                            li
                                a(href="#")
                                    i.fa.fa-tachometer.fa-fw
                                        .icon-bg.bg-orange
                                    span.menu-title DASHBOARD
                            li
                                a(href="#")
                                    i.fa.f.fa-sitemap.fa-fw
                                        .icon-bg.bg-dark
                                    span.menu-title Settings
                                        span.fa.arrow
                                ul.nav.nav-second-level
                                    li
                                        a(href='#')
                                            i.fa.fa-angle-right
                                            span.submenu-title Category
                                            span.fa.arrow
                                        ul.nav.nav-third-level
                                            li
                                                a(href='/category/createcategory')
                                                    i.fa.fa-angle-double-right
                                                    span.submenu-title Create Category
                                            li
                                                a(href='/category/categoryview')
                                                    i.fa.fa-angle-double-right
                                                    span.submenu-title Category View
                                    li
                                        a(href='#')
                                            i.fa.fa-angle-right
                                            span.submenu-title Brand
                                            span.fa.arrow
                                        ul.nav.nav-third-level
                                            li
                                                a(href='/brand/createbrand')
                                                    i.fa.fa-angle-double-right
                                                    span.submenu-title Create Brand
                                            li
                                                a(href='/brand/viewbrand')
                                                    i.fa.fa-angle-double-right
                                                    span.submenu-title Brand View
                                    li
                                        a(href='#')
                                            i.fa.fa-angle-right
                                            span.submenu-title Item
                                            span.fa.arrow
                                        ul.nav.nav-third-level
                                            li
                                                a(href='/item/createitem')
                                                    i.fa.fa-angle-double-right
                                                    span.submenu-title Create Item
                                            li
                                                a(href='/item/viewitem')
                                                    i.fa.fa-angle-double-right
                                                    span.submenu-title Item View
                block body
